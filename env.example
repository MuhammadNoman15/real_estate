# Copy this file to .env and fill values as needed

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=noman
POSTGRES_DB=real_estate_tool
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# App
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000

# External APIs
# If you use OpenAI for parsing/formatting, set your key here
OPENAI_API_KEY=
# Optional: choose a provider (openai, azure, anthropic, etc.)
LLM_PROVIDER=openai

# Google Maps (for geocoding/distance if provided)
GOOGLE_MAPS_API_KEY=

# TransLink (for transit data if provided)
TRANSLINK_API_KEY=

# JWT Authentication
# Generate a secure random key: openssl rand -hex 32
JWT_SECRET_KEY=your-secret-key-here-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30